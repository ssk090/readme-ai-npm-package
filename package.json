{
  "name": "@catmeow/readme-ai",
  "version": "1.0.1",
  "description": "AI-powered README generator that analyzes your project and creates comprehensive documentation",
  "main": "dist/index.js",
  "bin": {
    "readme-ai": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "start:server": "node dist/server.js",
    "dev": "tsc && node dist/cli.js",
    "dev:server": "tsc && node dist/server.js"
  },
  "keywords": [
    "readme",
